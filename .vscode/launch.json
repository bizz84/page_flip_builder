    "version": "0.2.0",
    "configurations": [
        {
            "name": "PageFlip mobile",
            "cwd": "example",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "PageFlip web",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "args": [
                "-d",
                "chrome"
            ]
        },
        {
            "name": "PageFlip web canvaskit",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "args": [
                "-d",
                "chrome",
                "--web-renderer",
                "canvaskit"
            ]
        },
        {
            "name": "Cards mobile",
            "cwd": "example",
            "program": "lib/main_cards.dart",
            "request": "launch",
            "type": "dart"
        }
    ]
}